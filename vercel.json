{
  "builds": [
    {
      "src": "**/*.php",
      "use": "vercel-php@0.7.2",
      "config": {
        "excludeFiles": [
          "assets/**"
        ]
      }
    },
    {
      "src": "css/**",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "pages/**",
      "use": "@vercel/static"
    }
  ]
}